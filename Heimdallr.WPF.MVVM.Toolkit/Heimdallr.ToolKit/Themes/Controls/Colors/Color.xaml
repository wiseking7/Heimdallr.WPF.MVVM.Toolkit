  <Color x:Key="SelectedColor">#FF3498DB</Color>
  <Color x:Key="BorderColor">#FFBDC3C7</Color>
  
  <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColorLight}" />
  <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
  <SolidColorBrush x:Key="HoverBrush" Color="{StaticResource HoverColor}" />
  <SolidColorBrush x:Key="SelectedBrush" Color="{StaticResource SelectedColor}" />
